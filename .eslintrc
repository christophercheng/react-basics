{
  "extends": "airbnb",
  "plugins": ["jest"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "ecmaVersion": 8,
      "jsx": true
    }
  },
  "env": {
  	"jest/globals": true,
  	"browser": true,
  	"node": true
  },
  "rules": {
    "no-unused-vars": ["error", {"argsIgnorePattern":"next"}],
    "jsx-a11y/anchor-is-valid":["error", {"argsIgnorePattern":"Link"}],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": [0],
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}